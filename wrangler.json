{
  "name": "svg-to-png",
  "compatibility_date": "2025-08-20",
  "main": "build/worker/shim.mjs",
  "build": {
    "command": "cargo install -q worker-build && worker-build --release",
    "upload": {
      "dir": "build/worker",
      "format": "modules"
    }
  },
  "rules": [
    {
      "globs": ["**/*.wasm"],
      "type": "CompiledWasm"
    }
  ]
}
